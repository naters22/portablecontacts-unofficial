application: facebook-portablecontacts
version: 1
threadsafe: false
api_version: 1
default_expiration: 1d

# ideally python27 but tweepy is incompatible. :/ details in app.yaml.twitter.
runtime: python27

handlers:
- url: /static
  static_dir: static

- url: /(.well-known/.*)?
  script: app.py
  secure: always

- url: /(start_auth|oauth_callback)
  script: facebook_demo.py
  secure: always

- url: /poco(/.*)?
  script: poco.py
  secure: always
