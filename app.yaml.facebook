application: facebook-portablecontacts
version: 1
# ideally python27 but tweepy is incompatible. :/ details in app.yaml.twitter.
runtime: python
threadsafe: false
api_version: 1
default_expiration: 1d

handlers:
- url: /static
  static_dir: static

- url: /(facebook_oauth_callback)?
  script: facebook_demo.py
  secure: always

- url: /.well-known/host-meta(\.xrds)?
  script: app.py
  secure: always

- url: /poco(/.*)?
  script: poco.py
  secure: always
