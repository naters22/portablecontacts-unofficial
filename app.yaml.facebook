application: facebook-portablecontacts
version: 1
runtime: python27
threadsafe: false
api_version: 1
default_expiration: 1d

handlers:
- url: /static
  static_dir: static

- url: /(facebook_oauth_callback)?
  script: facebook_demo.py
  secure: always

- url: /.well-known/host-meta(\.xrds)?
  script: app.py
  secure: always

- url: /poco(/.*)?
  script: poco.py
  secure: always
